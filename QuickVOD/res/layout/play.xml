<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
	android:id="@+id/widget62"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:background="@drawable/background"
	xmlns:android="http://schemas.android.com/apk/res/android"
	>
<ImageView 
	android:layout_height="wrap_content" 
	android:layout_width="wrap_content" 
	android:id="@+id/widget57" 
	android:src="@drawable/bottom" 
	android:layout_alignParentBottom="true"  
	android:layout_centerHorizontal="true"/>
<ImageView 
	android:layout_height="wrap_content" 
	android:layout_width="wrap_content" 
	android:id="@+id/widget64" 
	android:src="@drawable/middle_back"
	android:layout_centerVertical="true" 
	android:layout_alignRight="@+id/key_forward" />
<ImageView 
	android:src="@drawable/circle1" 
	android:id="@+id/circle" 
	android:layout_height="wrap_content" 
	android:layout_width="wrap_content" 
	android:layout_alignLeft="@+id/widget66" 
	android:layout_alignTop="@+id/widget66" />
<ImageView 
	android:layout_height="wrap_content" 
	android:background="#00000000" 
	android:layout_width="wrap_content" 
	android:id="@+id/key_close" 
	android:src="@drawable/close" 
	android:layout_alignParentBottom="true" 
	android:layout_alignLeft="@+id/widget64" 
	android:layout_marginLeft="29dip" />
<ImageView 
	android:layout_height="wrap_content" 
	android:background="#00000000" 
	android:layout_width="wrap_content" 
	android:id="@+id/key_return" 
	android:src="@drawable/revert" 
	android:layout_alignParentBottom="true" 
	android:layout_alignRight="@+id/widget64" 
	android:layout_marginRight="21dip" />
<ImageView 
	android:layout_height="wrap_content" 
	android:layout_width="wrap_content" 
	android:id="@+id/widget66" 
	android:src="@drawable/middle" 
	android:layout_alignTop="@+id/widget97" 
	android:layout_marginTop="7dip" 
	android:layout_centerHorizontal="true" />
<ImageView 
	android:layout_height="wrap_content" 
	android:layout_width="wrap_content" 
	android:id="@+id/widget97" 
	android:src="@drawable/right_state"
	android:layout_centerVertical="true" 
	android:layout_centerHorizontal="true" />
<ImageView 
	android:layout_height="wrap_content" 
	android:background="#00000000" 
	android:layout_width="wrap_content" 
	android:id="@+id/key_play" 
	android:src="@drawable/pause"
	android:layout_alignBottom="@+id/widget66" 
	android:layout_centerHorizontal="true" 
	android:layout_marginBottom="25dp" />
<TextView 
	android:textSize="20sp" 
	android:textStyle="bold" 
	android:layout_height="33dp" 
	android:text="" 
	android:layout_width="100dp" 
	android:id="@+id/speed" 
	android:layout_below="@+id/widget64" 
	android:layout_alignRight="@+id/widget64" />
<ImageView 
	android:src="@drawable/forward" 
	android:id="@+id/key_forward" 
	android:background="#00000000" 
	android:layout_height="wrap_content" 
	android:layout_width="wrap_content" 
	android:layout_alignBottom="@+id/key_play" 
	android:layout_toRightOf="@+id/widget97" />
<ImageView 
	android:src="@drawable/backword" 
	android:id="@+id/key_backword" 
	android:background="#00000000" 
	android:layout_height="wrap_content" 
	android:layout_width="wrap_content" 
	android:layout_alignBottom="@+id/key_play" 
	android:layout_toLeftOf="@+id/widget97" />
<TextView 
	android:layout_height="40dip" 
	android:textStyle="bold" 
	android:gravity="center" 
	android:textSize="25sp" 
	android:id="@+id/display" 
	android:text="" 
	android:layout_width="320dip"  
	android:layout_alignLeft="@+id/widget57" 
	android:layout_marginTop="14dp" />
</RelativeLayout>

