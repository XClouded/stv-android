<ImageView
    android:id="@+id/flower"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_x="28dp"
    android:layout_y="402dp"
    android:background="#00000000"
    android:src="@drawable/flower" />

