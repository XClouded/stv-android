<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout
	android:id="@+id/widget62"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:background="@drawable/background"
	xmlns:android="http://schemas.android.com/apk/res/android"
	>
<ImageView
	android:id="@+id/widget57"
	android:layout_width="320px"
	android:layout_height="111px"
	android:src="@drawable/bottom"
	android:layout_x="0px"
	android:layout_y="321px"
	/>
<ImageView
	android:id="@+id/widget64"
	android:layout_width="wrap_content"
	android:layout_height="wrap_content"
	android:src="@drawable/middle_back"
	android:layout_x="10px"
	android:layout_y="29px"
	/>
<ImageView
	android:id="@+id/widget66"
	android:layout_width="wrap_content"
	android:layout_height="wrap_content"
	android:src="@drawable/middle"
	android:layout_x="82px"
	android:layout_y="96px"
	/>
<ImageView
	android:id="@+id/key_ok"
	android:layout_width="wrap_content"
	android:layout_height="wrap_content"
	android:src="@drawable/ok"
	android:layout_x="123px"
	android:layout_y="156px"
	android:background="#00000000"
	/>
<ImageView
	android:id="@+id/widget97"
	android:layout_width="wrap_content"
	android:layout_height="wrap_content"
	android:src="@drawable/left_state"
	android:layout_x="73px"
	android:layout_y="89px"
	/>
<ImageView 
    android:src="@drawable/up" 
    android:layout_height="wrap_content" 
    android:layout_width="wrap_content" 
    android:id="@+id/key_up" 
    android:background="#00000000" 
    android:layout_x="132dp" 
    android:layout_y="30dp" 
    />
<ImageView 
    android:src="@drawable/right" 
    android:layout_height="wrap_content" 
    android:layout_width="wrap_content" 
    android:id="@+id/key_right" 
    android:background="#00000000" 
    android:layout_x="247dp" 
    android:layout_y="146dp" 
    />
<ImageView 
    android:src="@drawable/left" 
    android:layout_height="wrap_content" 
    android:layout_width="wrap_content" 
    android:id="@+id/key_left" 
    android:background="#00000000" 
    android:layout_x="14dp" 
    android:layout_y="146dp" 
    />
<ImageView 
    android:src="@drawable/down" 
    android:layout_height="wrap_content" 
    android:layout_width="wrap_content" 
    android:id="@+id/key_down" 
    android:background="#00000000" 
    android:layout_x="132dp" 
    android:layout_y="261dp" 
    />
<TextView 
    android:text="正在登陆系统,请稍后......" 
    android:gravity="center" 
    android:textSize="20sp" 
    android:textStyle="bold" 
    android:id="@+id/loginfo" 
    android:layout_width="320px" 
    android:layout_height="40dp" 
    android:layout_x="0dp" 
    android:layout_y="1dp" 
    />

<ImageView
    android:id="@+id/flower"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_x="25dp"
    android:layout_y="345dp"
    android:background="#00000000"
    android:src="@drawable/flower" />

<ImageView
    android:id="@+id/key_return"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_x="129dp"
    android:layout_y="340dp"
    android:background="#00000000"
    android:src="@drawable/revert" />

</AbsoluteLayout>
