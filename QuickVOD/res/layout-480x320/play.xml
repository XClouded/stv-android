<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout
	android:id="@+id/widget62"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:background="@drawable/background"
	xmlns:android="http://schemas.android.com/apk/res/android"
	>
<TextView 
	android:gravity="center" 
	android:text="" 
	android:textSize="25sp" 
	android:textStyle="bold"
	android:id="@+id/display" 
	android:layout_width="320px" 
	android:layout_height="wrap_content" 
	android:layout_x="0dp" 
	android:layout_y="15dp" 
	/>
<ImageView
	android:id="@+id/widget57"
	android:layout_width="320px"
	android:layout_height="111px"
	android:src="@drawable/bottom"
	android:layout_x="0px"
	android:layout_y="321px"
	/>
<ImageView
	android:id="@+id/widget64"
	android:layout_width="wrap_content"
	android:layout_height="wrap_content"
	android:src="@drawable/middle_back"
	android:layout_x="10px"
	android:layout_y="29px"
	/>
<ImageView
	android:id="@+id/circle"
	android:layout_width="wrap_content"
	android:layout_height="wrap_content"
	android:layout_x="85px"
	android:layout_y="100px"
	/>
<ImageView
	android:id="@+id/widget66"
	android:layout_width="wrap_content"
	android:layout_height="wrap_content"
	android:src="@drawable/middle"
	android:layout_x="82px"
	android:layout_y="96px"
	/>
<ImageView
	android:id="@+id/key_play"
	android:layout_width="wrap_content"
	android:layout_height="wrap_content"
	android:src="@drawable/pause"
	android:layout_x="142px"
	android:layout_y="140px"
	android:background="#00000000"
	/>
<ImageView
	android:id="@+id/widget97"
	android:layout_width="wrap_content"
	android:layout_height="wrap_content"
	android:src="@drawable/right_state"
	android:layout_x="73px"
	android:layout_y="89px"
	/>
<ImageView 
    android:layout_width="wrap_content" 
    android:id="@+id/key_forward" 
    android:background="#00000000" 
    android:src="@drawable/forward" 
    android:layout_height="wrap_content" 
    android:layout_x="252dp" 
    android:layout_y="150dp" 
    />
<ImageView 
    android:layout_width="wrap_content" 
    android:id="@+id/key_backword" 
    android:background="#00000000" 
    android:src="@drawable/backword" 
    android:layout_height="wrap_content" 
    android:layout_x="10dp" 
    android:layout_y="150dp" 
    />
<TextView 
    android:text="" 
    android:textSize="20sp" 
    android:textStyle="bold" 
    android:id="@+id/speed" 
    android:layout_height="33dp" 
    android:layout_y="306dp" 
    android:layout_width="102dp" 
    android:layout_x="214dp" 
    />
<ImageView
    android:id="@+id/flower"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_x="27dp"
    android:layout_y="345dp"
    android:background="#00000000"
    android:src="@drawable/flower" />

<ImageView
    android:id="@+id/key_return"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_x="132dp"
    android:layout_y="338dp"
    android:background="#00000000"
    android:src="@drawable/revert" />
</AbsoluteLayout>
