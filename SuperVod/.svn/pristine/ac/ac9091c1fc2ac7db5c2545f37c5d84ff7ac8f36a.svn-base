<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Button">
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textStyle">bold</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:background">@drawable/btn_long_bg</item>
    </style>

    <style name="further_title_button">
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textStyle">bold</item>
        <item name="android:padding">5dp</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:background">@drawable/btn_long_bg</item>
    </style>

    <style name="Title">
        <item name="android:textSize">23sp</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:layout_gravity">center</item>
    </style>

    <style name="List_Layout">
        <item name="android:layout_width">320dp</item>
        <item name="android:layout_height">103dp</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:paddingLeft">3dip</item>
    </style>

    <style name="List_Layout1">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">93dp</item>
        <item name="android:orientation">vertical</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:paddingTop">5dp</item>
    </style>

    <style name="List_Layout2">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:paddingTop">5dp</item>
    </style>

    <style name="List_Image">
        <item name="android:layout_width">72dp</item>
        <item name="android:layout_height">93dp</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:layout_gravity">center</item>
    </style>

    <style name="List_Movie_Name">
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="List_Movie_Text">
        <item name="android:text">类型:</item>
        <item name="android:textSize">17sp</item>
        <item name="android:textColor">#9b999a</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="List_Movie_Type">
        <item name="android:text"></item>
        <item name="android:textSize">17sp</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingLeft">5dp</item>
    </style>

    <style name="List_RatingBar">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingTop">8dp</item>
        <item name="android:isIndicator">true</item>
        <item name="android:numStars">10</item>
        <item name="android:progressDrawable">@drawable/ratingbar_self</item>
        <item name="android:minHeight">14dip</item>
        <item name="android:maxHeight">14dip</item>
    </style>

    <style name="Int_Score">
        <item name="android:textSize">25sp</item>
        <item name="android:textColor">#f88305</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:gravity">top</item>
        <item name="android:paddingLeft">3dp</item>
    </style>

    <style name="Float_Score">
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">#f88305</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_marginTop">5dp</item>
    </style>

    <style name="Text_Score">
        <item name="android:text">分</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingLeft">3dp</item>
    </style>

    <style name="Detail_Movie_Name">
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="Detail_Txt">
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">2dp</item>
    </style>

    <style name="Detail_Poster">
        <item name="android:layout_width">87dp</item>
        <item name="android:layout_height">116dp</item>
        <item name="android:scaleType">fitXY</item>
        <item name="android:layout_margin">5dp</item>
    </style>

    <style name="Detail_Info">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textStyle">normal</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">left|center_vertical</item>
    </style>

    <style name="Detail_Button">
        <item name="android:textSize">17sp</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textStyle">bold</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:background">@color/gray</item>
    </style>

    <style name="reg_textview">
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_width">120dp</item>
        <item name="android:layout_height">35dp</item>
        <item name="android:gravity">right</item>
        <item name="android:layout_marginTop">5dp</item>
    </style>

    <style name="reg_edittext">
        <item name="android:textSize">15sp</item>
        <item name="android:layout_width">180dp</item>
        <item name="android:layout_height">35dp</item>
        <item name="android:gravity">left</item>
        <item name="android:layout_marginTop">5dp</item>
    </style>
    
	<style name="Dialog" parent="@android:style/Theme.Dialog">
		<item name="android:windowFrame">@null</item><!--边框-->
        <item name="android:windowIsFloating">true</item><!--是否浮现在activity之上-->
        <item name="android:windowIsTranslucent">false</item><!--半透明-->
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@color/transparent</item>
        <item name="android:backgroundDimEnabled">false</item><!--模糊-->
        <!--<item name="android:windowAnimationStyle">@+android:style/Animation.Translucent</item>-->
    </style>
</resources>