<?xml version="1.0" encoding="utf-8"?>
    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/linearLayout1"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent" android:orientation="vertical">

        <RelativeLayout
            android:id="@+id/relativeLayout1"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent" >

            <GridView
                android:id="@+id/framework_bar"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:numColumns="5" android:layout_alignParentBottom="true">
            </GridView>

            <ViewFlipper
                android:id="@+id/framework_view"
                android:layout_width="fill_parent" android:layout_height="fill_parent" android:layout_above="@id/framework_bar" android:layout_alignParentTop="true">
            </ViewFlipper>

        </RelativeLayout>

    </LinearLayout>
